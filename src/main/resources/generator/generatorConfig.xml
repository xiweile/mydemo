<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
     "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
     
<generatorConfiguration>
	<!-- 数据库驱动包位置 -->
<!--	 <classPathEntry  location="/Users/mac/Documents/jar/mysql/mysql-connector-java.jar"/>-->
	<classPathEntry  location="E:/resource/ssm_jar/mysql-connector-java-5.1.26/mysql-connector-java.jar"/>
	<context id="context1">
	
	  	<commentGenerator>
	    	<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
	    	<property name="suppressAllComments" value="true"/>
 	 	</commentGenerator>
 	 	
		<!-- 数据库链接URL、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.10.8.10:3306/fwyh" userId="root" password="123456" />
			
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.foresee.fwyh.entity" targetProject="src/main/java" />
		
		<!-- 生成的映射文件报名和位置 -->
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources" />
		
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator targetPackage="com.foresee.fwyh.dao" targetProject="src/main/java" type="XMLMAPPER" />
		
		<!-- 要生成的那些表(更改tableName 和domainObjectName 就可以了) -->
		<table tableName="yh_yhzc_nsr_ycxx" domainObjectName="YhNsrYcxx"
                enableCountByExample="false"
                enableDeleteByExample="false"
                enableSelectByExample="false"
                enableUpdateByExample="false"
        ></table>
	</context>
</generatorConfiguration>
